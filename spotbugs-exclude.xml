<FindBugsFilter
        xmlns="https://github.com/spotbugs/filter/3.0.0"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="https://github.com/spotbugs/filter/3.0.0 https://raw.githubusercontent.com/spotbugs/spotbugs/3.1.0/spotbugs/etc/findbugsfilter.xsd">

    <Match>
        <Class name="io.oigres.ecomm.bff.admin.api.model.BrandWithDetailRequest"/>
        <Bug pattern="EI_EXPOSE_REP" />
    </Match>

    <Match>
        <Class name="io.oigres.ecomm.bff.admin.api.model.DashboardResponse"/>
        <Bug pattern="EI_EXPOSE_REP" />
    </Match>

    <Match>
        <Class name="io.oigres.ecomm.bff.admin.config.BackendServicesProperties"/>
        <Bug pattern="EI_EXPOSE_REP" />
    </Match>

    <Match>
        <Class name="io.oigres.ecomm.bff.admin.config.BackendServicesProperties"/>
        <Bug pattern="EI_EXPOSE_REP2" />
    </Match>

    <Match>
        <Class name="io.oigres.ecomm.bff.admin.api.model.CatalogProductByIdResponse"/>
        <Bug pattern="EI_EXPOSE_REP2" />
    </Match>

    <Match>
        <Class name="~io\.oigres\.ecomm\.bff\.admin\.config.*"/>
        <Bug pattern="EI_EXPOSE_REP" />
    </Match>

    <Match>
        <Class name="~io\.oigres\.ecomm\.bff\.admin\.config.*"/>
        <Bug pattern="EI_EXPOSE_REP2" />
    </Match>

    <Match>
        <Package name="~io\.oigres\.ecomm\.bff\.admin.*"/>
        <Bug pattern="EI_EXPOSE_REP2" />
    </Match>

</FindBugsFilter>
